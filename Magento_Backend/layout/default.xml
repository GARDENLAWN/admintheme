<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="css/gardenlawn-admin.css"/>
    </head>
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_image_src" xsi:type="string">images/logo-sygnet-transparent.svg</argument>
            </arguments>
        </referenceBlock>
        <move element="notification.messages" destination="header" before="user"/>
    </body>
</page>
